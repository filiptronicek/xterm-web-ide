{
    "name": "xterm",
    "kind": "browser",
    "version": "1.0.0",
    "title": "Terminal",
    "icon": "xterm/icon.svg",
    "entrypoint": "/ide/xterm/startup.sh",
    "readinessProbe": {
        "type": "http",
        "http": {
            "path": "/"
        }
    },
    "env": {
        "EDITOR": "/usr/bin/nano",
        "VISUAL": "$EDITOR",
        "GP_OPEN_EDITOR": "$EDITOR",
        "GIT_EDITOR": "$EDITOR",
        "GP_EXTERNAL_BROWSER": "/ide/xterm/startup.sh --openExternal --port 23000 ${term.pid}",
        "GP_PREVIEW_BROWSER": "$GP_EXTERNAL_BROWSER"
    }
}
